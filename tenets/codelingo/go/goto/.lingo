tenets:
  - name: goto-statement
    doc: |
      Find uses of goto.
    bots:
      codelingo/review:
        comments: goto statments have a niche use and can obscure the flow of a program.
    query: |
      import (
        codelingo/ast/go/0.0.0
      )

      @ review.comment
      go.branch_stmt({depth: any}):
        token: "goto"
