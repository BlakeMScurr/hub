tenets:
  - name: go-lint-rules
    doc: |
      Find all go linter rules.
    bots:
      codelingo/review:
        comments: |
          {{text}}
    query: |
      import (
        codelingo/ast/golint/0.0.0
      )

      @ review.comment
      golint.lint_rule({depth: any}):
        @ clair.vars.text
        text

